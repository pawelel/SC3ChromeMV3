{
    "manifest_version": 3,
    "name": "SC3 form filler",
    "version": "0.0.1",
    "description": "SC3 form filler using Tabulator",
    "homepage_url": "https://sc3-pl.vwgroup.com/sc3/index.do?lang=en",
    "action": {
        "default_title": "Service Center 3 form filler",
        "default_popup": "popup.html",
        "default_icon": "./icons/Hcl.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [{
        "resources": ["injector.js"],
        "matches": [ "*://sc3-pl.vwgroup.com/*",
        "*://sc3-pl.vw.vwg/*",
        "*://sc3.vwgroup.com/*",
        "*://sc3.vw.vwg/*"]
    }],
    "permissions": [
        "scripting",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "*://sc3-pl.vwgroup.com/*",
        "*://sc3-pl.vw.vwg/*",
        "*://sc3.vwgroup.com/*",
        "*://sc3.vw.vwg/*"
    ]
}